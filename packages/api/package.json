{
	"name": "@codeeditorland/api",
	"version": "0.0.1",
	"private": false,
	"description": "Tauri API definitions",
	"keywords": [
		"codeeditorland",
		"land",
		"playform"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/Tauri#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/Tauri/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/Tauri.git"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/tauri"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "🖋️ Source — 👐🏻 Open —",
		"email": "Source/Open@Editor.Land",
		"url": "HTTPS://Editor.Land"
	},
	"contributors": [
		"Tauri Programme within The Commons Conservancy"
	],
	"type": "module",
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs",
			"types": "./index.d.ts"
		},
		"./*": {
			"import": "./*.js",
			"require": "./*.cjs",
			"types": "./*.d.ts"
		},
		"./package.json": "./package.json"
	},
	"main": "./index.cjs",
	"module": "./index.js",
	"types": "./index.d.ts",
	"scripts": {
		"build": "rollup -c --configPlugin typescript",
		"eslint:check": "eslint src/**/*.ts",
		"eslint:fix": "eslint src/**/*.ts --fix",
		"npm-pack": "pnpm build && cd ./dist && npm pack",
		"npm-publish": "pnpm build && cd ./dist && pnpm publish --access public --loglevel silly --no-git-checks",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"ts:check": "tsc --noEmit"
	},
	"dependencies": {},
	"devDependencies": {
		"@playform/build": "0.2.1",
		"@types/node": "^22.10.5",
		"fast-glob": "3.3.3",
		"globals": "^15.14.0",
		"tslib": "^2.8.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
