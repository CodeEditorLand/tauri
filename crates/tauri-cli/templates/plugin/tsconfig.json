{
	"compilerOptions": {
		"baseUrl": "./",
		"module": "esnext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"outDir": "Target",
		"rootDir": "src",
		"skipLibCheck": true,
		"strict": true,
		"target": "es2021"
	},
	"exclude": [
		"dist-js",
		"node_modules"
	],
	"extends": "@playform/build/tsconfig",
	"include": [
		"guest-js/*.ts",
		"src"
	]
}
